{
  "name": "study-debate-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3010",
    "build:anlz": "rimraf .next && cross-env ANALYZE=true NODE_ENV=development next build",
    "build:dev": "rimraf .next && env-cmd -f .env.development next build",
    "build:stg": "rimraf .next &&  env-cmd -f .env.staging next build",
    "build:prod": "rimraf .next && env-cmd -f .env.production next build",
    "start": "next start -p 3010",
    "test": "jest --watch",
    "pre-commit": "lint-staged",
    "lint": "eslint --quiet --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint --fix --quiet --ext .ts,.tsx,.js,.jsx",
    "prepare": "husky install",
    "pm2:start": "pm2-runtime start ./ecosystem.config.js"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "cookies": "^0.8.0",
    "cors": "^2.8.5",
    "date-fns": "^2.22.1",
    "immer": "^9.0.3",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "next": "11.0.1",
    "next-redux-saga": "^4.1.2",
    "next-redux-wrapper": "^7.0.2",
    "next-seo": "^4.24.0",
    "querystring": "^0.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.8.7",
    "react-redux": "^7.2.4",
    "react-toastify": "^7.0.4",
    "react-use": "^17.2.4",
    "redux": "^4.1.0",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.3.0",
    "swr": "^0.5.6",
    "typesafe-actions": "^5.1.0"
